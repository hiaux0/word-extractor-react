{
  "manifest_version": 2,
  "name": "word-extractor",
  "description": "description",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "background": {
    "scripts": ["background_script.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/assets/contentScript.js", "dist/assets/browserAction.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "dist/index-browser-action.html",
    "default_title": "word-extractor"
  },
  "page_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "pageAction/index.html",
    "default_title": "word-extractor"
  },
  "options_ui": {
    "page": "options/index.html"
  },
  "permissions": ["activeTab", "contextMenus", "<all_urls>"]
}

