{
  "manifest_version": 2,
  "name": "word-extractor",
  "description": "description",
  "version": "0.1.0",
  "background": {
    "scripts": ["background/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/contentScript.js", "dist/assets/index.js"],
      "css": ["dist/index.css"]
    }
  ],
  "browser_action": {
    "default_title": "word-extractor-ba"
  },
  "permissions": ["activeTab", "contextMenus", "<all_urls>", "storage", "tabs"]
}
